{
    "extends": "./tsconfig.json",
    "include": [
        "../src/**/*",
        "../*.mjs",
        "../*.js",
        "../*.ts",
        "../*.json",
        "../.*.js",
        "../tools/**/*",
        "../tools/**/.*.js",
        "../tools/**/*.json",
        "../packages/**/*.json",
        "../demo-*/*.json",
        "../demo-ng/src/**/*.ts",
        "../demo-react/src/**/*.ts",
        "../demo-react/src/**/*.tsx",
        "../demo-svelte/src/**/*.ts",
        "../demo-svelte/src/**/*.svelte",
        "../demo-vue*/src/**/*.ts",
        "../demo-vue*/src/**/*.vue",
        "../demo-snippets/react/**/*.tsx",
        "../demo-snippets/react/**/*.ts",
        "../demo-snippets/vue*/**/*.vue",
        "../demo-snippets/vue*/**/*.ts",
        "../demo-snippets/svelte/**/*.svelte",
        "../demo-snippets/svelte/**/*.ts",
    ],
    "exclude": ["node_modules", "platforms"]
}
